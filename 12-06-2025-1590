

// So trying to remove the smallest subarray so that the sum of all
        // numbers is divisible by p First step is to aggregate da list, then
        // set up an increasing window that looks at X, and if the entries in
        // the list add up to the remainder, that's your answer
    int minSubarray(vector<int>& nums, int p) {
        
        int sum = accumulate(nums.begin(), nums.end(), 0);
        int tof = sum % p;
        for (int i = 0; i < nums.size(); i++) {
            for (int j = 0; j < nums.size() - i+1; j++) {
                if (accumulate(&nums[j], &nums[j + i], 0) == tof)
                    return i;
            }
        }
        return -1;
    }
};
